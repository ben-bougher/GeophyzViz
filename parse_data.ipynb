       "   u'Shell International E & P Inc.'],\n",
       "  u'issue': [u'4'],\n",
       "  u'keywords': [],\n",
       "  u'title': [u'Improving the virtual source method by wavefield separation'],\n",
       "  u'volume': [u'72'],\n",
