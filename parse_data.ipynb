       "   u'Laboratoire Central des Ponts et Chauss\\xe9es',\n",
       "   u'Laboratoire Central des Ponts et Chauss\\xe9es'],\n",
       "  u'issue': [u'3'],\n",
       "  u'keywords': [],\n",
       "  u'title': [u'Nonlinear inversion of geoelectric data acquired across 3D objects using a finite-element approach'],\n",
       "  u'volume': [u'73'],\n",
