       "  u'doi': [u'http://dx.doi.org/10.1190/1.2783766'],\n",
       "  u'institution': [u'Central University of Venezuela',\n",
       "   u'Petrobaras Energ\\xeda Venezuela'],\n",
       "  u'issue': [u'6'],\n",
       "  u'keywords': [],\n",
       "  u'title': [u'A Monte Carlo approach to the joint estimation of reservoir and elastic parameters from seismic amplitudes'],\n",
